<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.davizn.model.interfaces.PersonalDataDAO">

	<!-- 데이터 추가 -->
	<insert id="addPersonalData" parameterType="kr.or.davizn.model.dto.PersonalDataDTO">
		insert into personaldata values(data_seq.nextval,#{strgseq},#{dataname},#{datatype},sysdate)
	</insert>
	
	<!-- 데이터 수정 -->
	<select id="modifyPersonalData" parameterType="String" resultType="kr.or.davizn.model.dto.PersonalDataDTO">
		select * from personaldata where strgseq=#{strgseq}
	</select>
	
	<!-- 데이터 리스트 출력 -->
	<select id="showPersonalDataList" parameterType="String" resultType="kr.or.davizn.model.dto.PersonalDataDTO">
		select * from personaldata where strgseq=#{strgseq}
	</select>
</mapper>