<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.or.davizn.groupInterface.GroupShareDataDAO">
	<insert id="addShareData" parameterType="kr.or.davizn.groupDTO.GroupShareDataDTO">
		insert into groupsharedata(share_dataseq,groupseq,dataseq,userid,sharedate,datatype)
		values (sharedata_seq,#{groupseq},#{dataseq},#{userid},sysdate,#{datatype})
	</insert>
</mapper>

