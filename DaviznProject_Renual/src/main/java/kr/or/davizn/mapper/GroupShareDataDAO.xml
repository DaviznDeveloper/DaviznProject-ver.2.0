<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.or.davizn.groupInterface.GroupShareDataDAO">

	<insert id="addShareData" parameterType="kr.or.davizn.groupDTO.GroupShareDataDTO">
		insert into groupsharedata(share_dataseq,groupseq,dataseq,userid,sharedate,datatype,dataname)
		values (data_seq.nextval,#{groupseq},data_seq.nextval,#{userid},sysdate,#{datatype},#{dataname})
	</insert>
	
	<select id="showShareDataList" parameterType="Integer" resultType="kr.or.davizn.groupDTO.GroupShareDataDTO">
		select * from groupsharedata where groupseq=#{groupseq}
	</select>
</mapper>

